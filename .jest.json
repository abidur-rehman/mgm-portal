{
  "verbose": true,
    "transform": {
    "^.+\\.jsx?$": "babel-jest"
  },
  "testMatch": [
    "**/src/test/js/**/*-test.js"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
    }
  },
  "coverageReporters": ["text"]
}